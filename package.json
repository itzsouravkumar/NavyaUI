{
  "name": "navyaui",
  "version": "1.0.3",
  "description": "Premium animated UI components built with React and Tailwind CSS.",
  "type": "module",
  "main": "dist-lib/navyaui.cjs",
  "module": "dist-lib/navyaui.es.js",
  "types": "dist-lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist-lib/navyaui.es.js",
      "require": "./dist-lib/navyaui.cjs",
      "types": "./dist-lib/index.d.ts"
    }
  },
  "files": [
    "dist-lib/**/*.js",
    "dist-lib/**/*.cjs",
    "dist-lib/**/*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build:registry": "node scripts/build-registry.js",
    "build:lib": "vite build -c vite.config.lib.ts",
    "build:docs": "npm run build:registry && tsc -b && vite build",
    "build": "npm run build:lib && npm run build:docs",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@react-spring/web": "^10.0.3",
    "@tsparticles/engine": "^3.9.1",
    "@tsparticles/plugin-polygon-mask": "^3.9.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "clsx": "^2.1.1",
    "framer-motion": "^11.3.0",
    "lucide-react": "^0.424.0",
    "react-router-dom": "^6.26.0",
    "tailwind-merge": "^2.4.0",
    "webgl-fluid": "^0.3.9"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.5.3",
    "vite": "^5.3.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
